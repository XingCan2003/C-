#include <stdio.h>//

int main()//单分支和多分支
{
  int age = 70;
  if(age<18)
    printf("未成年\n");
  else  if(age>=18 && age<23)//可以在这里用else作为开头
      printf("青年\n");
  else if(age>=23 && age<50)
      printf("壮年\n");
  else if(age>=50 && age<90)
      printf("老人\n");
  else 
      printf("仙人\n");
  return 0;
}
/*else 用else做开头
  {
    if(age>=18 && age<23)
      printf("青年\n");
    else if(age>=23 && age<50)
      printf("壮年\n");
    else if(age>=50 && age<90)
      printf("老人\n");
    else 
      printf("仙人\n");
    }*/
