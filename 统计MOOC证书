#include <stdio.h>
int main()
{
    int n, x, i, a = 0, b = 0, c = 0;
    scanf("%d", &n);
    for (i = 0; i < n; i++)
    {
        scanf("%d", &x);
        if (x >= 85)
        {
            a++;
        }
        else if (x < 85 && x >= 60)
            b++;
        else
            c++;
    }
    printf("%d %d %d", a, b, c);
    return 0;
}
