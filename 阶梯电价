#include<stdio.h>
int main()
{
    float x,y;
    scanf("%f",&x);
    if( x>0 && x<=50)
    {
        y = 0.53*x;
        printf("cost = %.2f",y);
    }
    else if(x < 0)
    {
        printf("Invalid Value!");
    }
    else
    {
        y = 26.5 + (x-50)*0.58;
        printf("cost = %.2f",y);
    }
    return 0;
}
